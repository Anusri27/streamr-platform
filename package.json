        "start": "node scripts/start.js",
        "build": "node scripts/build.js",
        "test": "node scripts/test.js --env=jsdom",
        "test-e2e": "node scripts/test.js --config=./src/tests/e2e/config.js",
        "screenshots": "node scripts/test.js --config=./src/tests/e2e/config.js --testMatch='**/screenshots.js' ./tests/e2e/"
    },
    "proxy": {
        "/api": {
            "target": "http://localhost:8081",
            "pathRewrite": {
                "^/api": "streamr-core/api/v1"
            },
            "changeOrigin": true
        }
    },
    "babel": {
        "presets": [
            "react-app"
        ]
    },
    "jest": {
        "moduleNameMapper": {
            "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/tests/__mocks__/fileMock.js",
            "\\.(css|pcss)$": "identity-obj-proxy"
        },
        "setupFiles": [
            "<rootDir>/config/polyfills.js",
            "<rootDir>/src/tests/setup-tests.js"
        ],
        "collectCoverageFrom": [
            "src/**/*.{js,jsx,mjs}"
        ],
        "testMatch": [
            "<rootDir>/src/tests/**/?(*.)(spec|test).{js,jsx,mjs}"
        ],
        "testPathIgnorePatterns": [
            "/node_modules/"
        ],
        "moduleFileExtensions": [
            "web.js",
            "js",
            "json",
            "web.jsx",
            "jsx",
            "node",
            "mjs"
        ]
    },
    "devDependencies": {
        "expect-more-jest": "^2.0.0",
        "expect-puppeteer": "^3.2.0",
