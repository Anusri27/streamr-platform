
@import 'variables.pcss';

.loadingIndicator {
  top: 2px;
}

.content {
  background-color: #EFEFEF;
  padding: 48px 0 6rem 0;
  min-height: calc(100vh - 293px);
}

@media (--md-down) {
  .content {
    padding: 80px 0 6rem 0;
    min-height: calc(100vh - 277px);
  }
}

@media (--sm-down) {
  .content {
    padding: 32px 0 6rem 0;
    min-height: calc(100vh - 277px);
  }
}
